@model string[]
@{
    ViewBag.Title = "Registered Resource Sets";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="row">
    <div class="col s12 m6 offset-m3">
        <div class="col s12">
            <div class="card">
                <div class="card-content">
                    <h5>@ViewBag.Title</h5>
                    <div class="row">
                        <div class="col s12">
                            <ul class="collection">
                                @foreach (var id in Model)
                                {
                                    <li>
                                        <div class="col m4">@id</div>
                                        <div class="col m4">@Html.ActionLink("Manage", "GetResourceSetPolicy", "ResourceSet", new { id = id })</div>
                                    </li>
                                }
                            </ul>
                        </div>
                    </div>
                </div>
                <div class="card-action">
                </div>
            </div>
        </div>
    </div>
</div>
