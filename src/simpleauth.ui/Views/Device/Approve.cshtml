@{
	ViewBag.Title = "Authorization Complete";
	Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="row">
 <div class="col s12 m6 offset-m3">
  <div class="card">
   <div class="card-title"><h1>@ViewBag.Title</h1></div>
   <div class="card-image">
	<img alt="Device Authorization" src="~/img/manage_resources.png" />
   </div>
   <div class="card-action center">
	<div>Device authorization completed</div>
	<div>Please return to your device to complete the authorization.</div>
   </div>
  </div>
 </div>
</div>
